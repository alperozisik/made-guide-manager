    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^33.0.0",
    "html-webpack-plugin": "^5.6.0",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "style-loader": "^4.0.0",
    "wait-on": "^7.0.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.0"
