{
  "name": "made-guide-manager",
  "version": "1.0.0",
  "description": "MADE Guide Manager Electron Application",
  "main": "src/main.js",
  "scripts": {
    "start": "npm run build && electron .",
    "build": "webpack",
    "dev": "cross-env NODE_ENV=development webpack serve",
